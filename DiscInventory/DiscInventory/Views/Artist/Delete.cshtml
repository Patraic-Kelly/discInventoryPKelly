@model Artist
@{ 
    ViewBag.Title = "Delete Artist";
}
<h2>Confirm Delete</h2>
<h3>@Model.ArtistName @Model.ArtistLastName</h3>
<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="ArtistId" />

    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-controller="Artist" asp-action="Index" class="btn btn-primary">Return</a>
</form>